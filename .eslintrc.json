{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "extends": "airbnb",
    // "plugins": [
    //     "react"
    // ],
    "rules": {
        "indent": [1, "tab"],
        "comma-dangle": [2, "only-multiline"],
		"no-trailing-spaces": [2, { "skipBlankLines": true }]
    },
    "env": {
        "es6": true
    }
}