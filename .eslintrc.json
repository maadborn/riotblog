{
    "parserOptions": {
        "ecmaVersion": 6
    },
    "extends": "airbnb",
    // "plugins": [
    //     "react"
    // ],
    "rules": {
        "arrow-body-style": [0, "as-needed"],
        "comma-dangle": [2, "only-multiline"],
        "indent": [1, "tab"],
		"no-trailing-spaces": [2, { "skipBlankLines": true }],
        "space-before-function-paren": ["error", "never"]
    },
    "env": {
        "es6": true
    }
}